<?xml version="1.0" encoding="UTF-8" ?>
<beans
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <bean id="list" class="com.example.model.ListBean">
        <property name="list">
            <list value-type="java.lang.String">
                <value>1</value>
                <value>2</value>
                <value>3</value>
                <value>4</value>
                <value>5</value>
                <value>6</value>
            </list>
        </property>
    </bean>
    <bean id="set" class="com.example.model.ListBean">
        <property name="set">
            <set value-type="java.lang.String">
                <value>1</value>
                <value>2</value>
                <value>3</value>
                <value>4</value>
                <value>5</value>
                <value>6</value>
            </set>
        </property>
    </bean>
    <bean id="collection" class="com.example.model.ListBean">
        <property name="collection">
            <list value-type="java.lang.String">
                <value>1</value>
                <value>2</value>
                <value>3</value>
                <value>4</value>
                <value>5</value>
                <value>6</value>
            </list>
        </property>
    </bean>
</beans>
